  "scripts": {
    "start": "cross-env NODE_ENV=development node ./src/server/index.js",
    "build": "webpack --config ./config/webpack/production.js",
    "lint": "eslint --ext .js,.jsx ./config ./src",
    "test": "jest --coverage -u",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "prod": "cross-env NODE_ENV=production node ./src/server/index.js"
  },
  "jest": {
    "transform": {
      "^.+\\.js(x)?$": "./config/jest/transformer.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run stylelint"
    }
  },
